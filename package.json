{
  "name": "cmz",
  "version": "3.5.0",
  "description": "CSS Modules Zero",
  "main": "index.js",
  "dependencies": {},
  "bin": {
    "cmz-min": "bin/min.js"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "uglify-es": "^3.0.15"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm run clean",
    "build": "npm run build:umd && npm run build:minify",
    "build:umd": "browserify --standalone cmz -o dist/cmz.js index.js",
    "build:minify": "uglifyjs -o dist/cmz.min.js dist/cmz.js",
    "lint": "standard",
    "test": "tape tests/*.js | tap-spec",
    "pre-publish": "npm run lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwnj/cmz.git"
  },
  "standard": {
    "ignore": [
      "example"
    ]
  },
  "keywords": [],
  "author": "joshwnj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwnj/cmz/issues"
  },
  "homepage": "https://github.com/joshwnj/cmz#readme",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "tests"
  }
}
