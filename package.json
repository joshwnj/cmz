{
  "name": "cmz",
  "version": "2.2.0",
  "description": "CSS Modules Zero",
  "main": "index.js",
  "dependencies": {
    "caller": "^1.0.1"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "standard": "^9.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "uglifyjs": "^2.4.10"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm run clean",
    "build": "npm run build:umd && npm run build:minify",
    "build:umd": "browserify --standalone cmz -o dist/cmz.js index.js",
    "build:minify": "uglifyjs -o dist/cmz.min.js dist/cmz.js",
    "lint": "standard",
    "test": "tape tests/*.js | tap-spec",
    "prepublish": "npm run lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwnj/cmz.git"
  },
  "standard": {
    "ignore": [
      "example"
    ]
  },
  "keywords": [],
  "author": "joshwnj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwnj/cmz/issues"
  },
  "homepage": "https://github.com/joshwnj/cmz#readme"
}
